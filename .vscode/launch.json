    "version": "0.2.0",
    "configurations": [
        {
            "command": "npm start",
            "name": "Run Client application",
            "request": "launch",
            "type": "node-terminal",
            "cwd": "${workspaceFolder}/client"
        },
        {
            "name": "Debug Fedex Tracker",
            "request": "launch",
            "runtimeArgs": [
                "run-script",
                "debug-fedex-tracker"
            ],
            "runtimeExecutable": "npm",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node"
        },
        {
            "name": "Debug Fedex Parser",
            "request": "launch",
            "runtimeArgs": [
                "run-script",
                "debug-fedex-parser"
            ],
            "runtimeExecutable": "npm",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node"
        },
